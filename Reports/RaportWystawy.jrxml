	<queryString language="SQL">
		<![CDATA[SELECT wystawy.Tytul TytulWystawy, 
DataPocz, DataKon, dziela.Tytul TytulDziela,
dziela.Cena Cena, dziela.Rozmiar Rozmiar, 
kategorie.Nazwa Kategoria,
 CONCAT( kontrahenci.Imie, ' ' , kontrahenci.Nazwisko)  Autor
FROM wystawy, dziela, dzielo_wystawa, kontrahenci, kategorie
WHERE dzielo_wystawa.IdWystawy = wystawy.Id 
AND dzielo_wystawa.IdDziela = dziela.NrInw
AND dziela.IdAutora = kontrahenci.Id
AND dziela.IdKategorii = kategorie.Id
AND wystawy.Tytul =  $P{ParametrWystawa}
]]>
